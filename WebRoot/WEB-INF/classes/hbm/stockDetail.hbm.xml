<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="stock.pojo.StockVO" table="stock_deal_t">
		<id name="dealSno" column="DEAL_SNO" type="int">
			<generator class="sequence">
				<param name="sequence">STOCK_SNO_SEQ</param>
			</generator>
		</id>		
		<property name="dealStockCode" column="DEAL_STOCKSNO" type="string" />
		<property name="dealPrice" column="DEAL_PRICE" type="double" />
		<property name="dealStockName" column="DEAL_STOCKNAME" type="string" />
		<property name="dealStockNum" column="DEAL_NUM" type="int" />
		<property name="dealFee" column="DEAL_FEE" type="double" />
		<property name="dealTime" column="DEAL_TIME" type="date" />
		<property name="dealTax" column="DEAL_TAX" type="double" />
		<property name="dealIdea" column="DEAL_IDEA" type="string" />
		<property name="happyNum" column="HAPPY_NUM" type="int" />
		<property name="dealTinkPrice" column="DEAL_THINK_PRICE" type="double" />
		<property name="dealType" column="DEAL_TYPE" type="string" />
		<property name="configType" column="CONFIG_TYPE" type="int" />
	</class>
</hibernate-mapping>
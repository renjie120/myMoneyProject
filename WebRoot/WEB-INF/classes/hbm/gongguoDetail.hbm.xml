<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="tallyBook.pojo.GongguoDetail" table="gong_guo_detail">
		<id name="sno" column="id">
			<generator class="sequence">
				<param name="sequence">SEQ_GONGGUO</param>
			</generator>
		</id>		 
		<property name="content" column="GONGGUOTYPE" type="string" />
		<property name="time" column="SAVETIME" type="date" /> 
		<property name="desc" column="SOMETHING" type="string" /> 
	</class>
</hibernate-mapping>
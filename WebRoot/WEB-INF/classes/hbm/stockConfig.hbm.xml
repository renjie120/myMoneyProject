<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="stock.pojo.StockConfigVO" table="stock_config_t">
		<id name="configSno" column="CONFIG_SNO" type="int"></id>
		<property name="stockFee" column="STOCK_FEE" type="double" />
		<property name="taxFee" column="TAX_FEE" type="double" />
		<property name="configName" column="CONFIG_NAME" type="string" />
		<property name="minFee" column="MIN_MONEY" type="double" />
	</class>
</hibernate-mapping>